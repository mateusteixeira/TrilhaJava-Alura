package br.com.alura.programa;

import java.util.ArrayList;
import java.util.List;

import br.com.alura.banco.modelo.ContaPoupanca;

public class TestaCollections {

	public static void main(String[] args) {
		List<ContaPoupanca> contas = new ArrayList<>();
		ContaPoupanca contaPoupanca = new ContaPoupanca();
		contaPoupanca.setNumeroConta(3);
		contas.add(contaPoupanca);
		
		contaPoupanca = new ContaPoupanca();
		contaPoupanca.setNumeroConta(2);
		contas.add(contaPoupanca);
		contaPoupanca = new ContaPoupanca();
		contaPoupanca.setNumeroConta(103);
		contas.add(contaPoupanca);
		contaPoupanca = new ContaPoupanca();
		contaPoupanca.setNumeroConta(1);
		contas.add(contaPoupanca);
		contaPoupanca = new ContaPoupanca();
		contaPoupanca.setNumeroConta(6);
		contas.add(contaPoupanca);
		
	}
}
